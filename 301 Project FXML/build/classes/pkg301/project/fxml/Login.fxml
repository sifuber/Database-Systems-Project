<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" fillWidth="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="335.0" spacing="10.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pkg301.project.fxml.LoginController">
    <children>
        <Label fx:id="loginLabel" text="Log In">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="usernameField" alignment="CENTER" onAction="#loginButtonAction" promptText="Username" VBox.vgrow="ALWAYS" />
        <PasswordField fx:id="passwordField" alignment="CENTER" onAction="#loginButtonAction" promptText="Password" VBox.vgrow="ALWAYS" />
        <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginButtonAction" text="Log In" />
        <Button fx:id="registerButton" mnemonicParsing="false" onAction="#registerButtonAction" text="Create an Account" />
        <Region />
        <Button fx:id="guestLoginButton" mnemonicParsing="false" onAction="#guestLoginButtonAction" text="Guest Login" />
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
